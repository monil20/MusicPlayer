<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingsFragment">

    <Switch
        android:id="@+id/ralewayTextView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Play songs via SMS"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.034"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.016" />

    <com.example.monilandharia.musicplayer.customViews.RalewayTextView
        android:id="@+id/ralewayTextView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:text="@string/sms_info"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ralewayTextView6"
        app:layout_constraintTop_toBottomOf="@+id/ralewayTextView6"
        app:layout_constraintVertical_bias="0.0" />

    <Switch
        android:id="@+id/ralewayTextView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Enable gesture control"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ralewayTextView6"
        app:layout_constraintTop_toBottomOf="@+id/ralewayTextView5"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/ralewayTextView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Personalize"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ralewayTextView6"
        app:layout_constraintTop_toBottomOf="@+id/ralewayTextView7"
        app:layout_constraintVertical_bias="0.031" />

    <TextView
        android:id="@+id/ralewayTextView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Equalizer"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ralewayTextView6"
        app:layout_constraintTop_toBottomOf="@+id/ralewayTextView8"
        app:layout_constraintVertical_bias="0.074" />

    <TextView
        android:id="@+id/ralewayTextView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sleep time"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ralewayTextView6"
        app:layout_constraintTop_toBottomOf="@+id/ralewayTextView9"
        app:layout_constraintVertical_bias="0.091" />

</android.support.constraint.ConstraintLayout>